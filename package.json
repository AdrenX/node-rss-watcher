{
    "name": "rss-watcher",
    "version": "1.0.7",
    "description": "RSS reader/watcher",
    "main": "lib/watcher.js",
    "scripts": {
      "test": "mocha test/test.coffee | ./node_modules/coveralls/bin/coveralls.js",
      "test-cover": "mocha test/test.coffee -R html-cov > coverage.html",
      "test-spec":"mocha test/test.coffee --reporter spec"

    },
    "config": {
      "blanket": {
        "pattern": "lib",
        "loader": "./node-loaders/coffee-script",
        "data-cover-never": "node_modules"
      }
    },
    "bin": {
      "rss-watcher": "./bin/rss-watcher"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/nikezono/node-rss-watcher.git"
    },
    "dependencies": {
      "async": "~0.9.0",
      "colors": "~0.6.2",
      "commander": "~2.2.0",
      "goo.gl": "~0.1.0",
      "moment": "~2.7.0",
      "parse-rss": "*",
      "rss-frequency": "*"
    },
    "devDependencies": {
      "blanket": "^1.1.6",
      "coveralls": "^2.11.1",
      "mocha-lcov-reporter": "0.0.1",
      "coffee-script": "*",
      "mocha": "*"
    },
    "keywords": [
      "rss",
      "atom",
      "reader",
      "watcher",
      "watch",
      "read"
    ],
    "author": "nikezono",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/nikezono/node-rss-watcher/issues"
    }
}
